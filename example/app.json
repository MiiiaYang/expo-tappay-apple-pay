{
  "expo": {
    "name": "expo-tappay-apple-pay-example",
    "slug": "expo-tappay-apple-pay-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.stanma.tappay.applepay.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.stanma.tappay.applepay.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-tappay-direct",
      [
        "../app.plugin.js", {
          "merchantId": "merchant.tech.cherri.global.test",
          "acceptNetworks": [
            "Visa",
            "MasterCard"
          ],
          "appId": 11340,
          "appKey": "app_whdEWBH8e8Lzy4N6BysVRRMILYORF6UxXbiOFsICkz0J9j1C0JUlCHv1tVJC",
          "serverType": "sandbox"
        }
      ]
    ]
  }
}
